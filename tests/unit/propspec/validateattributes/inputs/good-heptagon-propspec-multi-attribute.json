    "batch_children_proc_ids": {
      "type": "obj",
      "title": "Batch submitted processes",
      "description": "Processes created by batch submission",
      "order": 0,
      "is_readonly": true,
      "is_autofill": true,
      "is_optional": true,
      "is_multi": true,
      "is_flattened_multi": true,
      "constraint": "type",
      "constraint_data": "svc-proc",
      "_definer_type": "process"
    },
    "batch_ignore_errors": {
      "type": "bool",
      "title": "Ignore errors in individual submitted processes",
      "description": "Continue batching even if some batched computations could not be launched",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "_definer_type": "process"
    },
    "batch_svc": {
      "type": "string",
      "title": "Batch Service",
      "description": "Determines if this tool should be run in single mode (one submission) or batch mode (multiple submissions).  NOTE: setting this option will clear other fields entered into the form.",
      "order": 0,
      "is_readonly": true,
      "is_submitonce": true,
      "is_optional": true,
      "constraint": "choice+",
      "constraint_data": "single\/\/\/Single Mode",
      "_definer_type": "process"
    },
    "children_proc_ids": {
      "type": "obj",
      "title": "Children Processes",
      "order": 0,
      "is_readonly": true,
      "is_autofill": true,
      "is_optional": true,
      "is_multi": true,
      "is_flattened_multi": true,
      "constraint": "type",
      "constraint_data": "svc-proc",
      "_definer_type": "process"
    },
    "countAAs": {
      "type": "bool",
      "default_value": "0",
      "title": "Compute AA profile",
      "description": "Compute Aminoacid rep[lacements  based on AA calls on reads",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "created": {
      "type": "datetime",
      "title": "Created",
      "order": 0,
      "is_readonly": true,
      "is_optional": true,
      "_definer_type": "base"
    },
    "cutEnds": {
      "type": "string",
      "default_value": "do not cut",
      "title": "Truncate Terminals",
      "description": "Truncate so many bases from alignment heads and tails",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "directionalityInfo": {
      "type": "bool",
      "default_value": "false",
      "title": "Print directionality details",
      "description": "Print forward and reverse ACGT In/Del counts and qualiy",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "_definer_type": "svc-profiler"
    },
    "disbalanceFR": {
      "type": "string",
      "default_value": "0",
      "title": "Forward/Reverse Disbalance",
      "description": "Automatic adjustment limit for basce-calls with forward/reverse expression disbalance",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "entrCutoff": {
      "type": "string",
      "default_value": "0",
      "title": "Entropic Cutoff",
      "description": "Decision to filter SNPs based on posiotnal base occurence entropy is made based on this value",
      "order": 0,
      "is_readonly": false,
      "constraint": "choice+",
      "constraint_data": "0\/\/\/Disabled|-1\/\/\/Review Only|0.95\/\/\/0.95|0.90\/\/\/0.90|0.75\/\/\/0.75|0.67\/\/\/0.67 One Sigma",
      "_definer_type": "svc-profiler"
    },
    "filterZeros": {
      "type": "integer",
      "default_value": "1",
      "title": "Treat filtered positions",
      "order": 0,
      "is_readonly": false,
      "constraint": "choice",
      "constraint_data": "1\/\/\/Ignore|0\/\/\/Fill with zeros",
      "_definer_type": "svc-profiler"
    },
    "generateIon": {
      "type": "bool",
      "default_value": "false",
      "title": "Generate annotation file",
      "description": "Generate file that can be used as annotation. Impacts the speed and the efficiency of the process",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "_definer_type": "svc-profiler"
    },
    "histograms": {
      "type": "integer",
      "default_value": "0",
      "title": "Tail Histograms",
      "order": 0,
      "is_readonly": false,
      "constraint": "choice",
      "constraint_data": "0\/\/\/Do not generate histograms|1\/\/\/Generate histograms",
      "_definer_type": "svc-profiler"
    },
    "lenPercCutoff": {
      "type": "string",
      "default_value": "no cutoff",
      "title": "Minimal Alignment Length",
      "description": "Minimal alignment length in percent of original query length accepted for profiling",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "maxLowQua": {
      "type": "string",
      "default_value": "no cutoff",
      "title": "Maximum Percentage of Low-Quality Regions",
      "description": "Overall quality filter of mapped sequences in percent of original query length",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "maxMissmatchPercCutoff": {
      "type": "string",
      "default_value": "0",
      "title": "Maximum Mismatch Percent",
      "description": "Maximum allowed missmatch percent for alignments to be considered for reference profiling",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "maxRptIns": {
      "type": "string",
      "default_value": "3",
      "title": "Count of Inserts Invalidating Repeats ",
      "description": "Maximum stuck letter base repeat after which in-dels are ignored",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "minCover": {
      "type": "string",
      "default_value": "10",
      "title": "Minimal Coverage Allowed",
      "description": "Regions which have coverage less than the specified amount are discarded from basecalling procedure",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "minImportantEntropy": {
      "type": "real",
      "default_value": "0.0",
      "title": "Safe Entropy Zone",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "modified": {
      "type": "datetime",
      "title": "Modified",
      "order": 0,
      "is_readonly": true,
      "is_optional": true,
      "_definer_type": "base"
    },
    "noise": {
      "type": "list",
      "title": "Noise",
      "description": "Parameters defining the behavor of the algorithm during noise treatments",
      "order": 0,
      "is_readonly": false,
      "is_flattened_decor": true,
      "_definer_type": "svc-profiler",
      "_children": {
        "noiseFilterBase": {
          "type": "obj",
          "title": "Noise Profiling Process ID",
          "description": "Process ID of the previous profiling computation",
          "order": 0,
          "is_readonly": false,
          "is_optional": true,
          "constraint": "type",
          "constraint_data": "svc-profiler+",
          "_definer_type": "svc-profiler"
            },
        "noiseFilterParams": {
          "type": "string",
          "default_value": "none",
          "title": "Noise Filtering Parameters",
          "description": "Determine the way the sequencing noise is treated for following variations: A-C, A-G, A-T, C-A, C-G, C-T, G-A, G-C, G-T, T-A, T-C, T-G",
          "order": 0,
          "is_readonly": false,
          "constraint": "choice",
          "constraint_data": "none\/\/\/Do not filter noise|auto\/\/\/Automatically filter 95 % noise|borrow\/\/\/Use the noise level of plasmid profiling experiment",
          "_definer_type": "svc-profiler"
            },
        "noiseProfileMax": {
          "type": "real",
          "default_value": "0.01",
          "title": "Noise Profile Maximum Cutoff",
          "order": 0,
          "is_readonly": false,
          "constraint": "eval",
          "constraint_data": "0.001<=$_(val) && $_(val)<1",
          "_definer_type": "svc-profiler"
            },
        "noiseProfileResolution": {
          "type": "real",
          "default_value": "0.0001",
          "title": "Noise Profile Resolution",
          "description": "Reslution to output the noise profile",
          "order": 0,
          "is_readonly": false,
          "constraint": "eval",
          "constraint_data": "0.0001<=$_(val) && $_(val)<0.1",
          "_definer_type": "svc-profiler"
            }
        }
    },
    "parent_proc_ids": {
      "type": "obj",
      "title": "Alignment(s)",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "is_multi": true,
      "is_flattened_multi": true,
      "is_batch": true,
      "constraint": "type",
      "constraint_data": "process+",
      "_definer_type": "svc-profiler"
    },
    "profSelector": {
      "type": "string",
      "title": "Prof Tool Selector",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "is_hidden": true,
      "_definer_type": "svc-profiler"
    },
    "random_seed": {
      "type": "integer",
      "default_value": "0",
      "title": "Random Seed",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "process"
    },
    "referenceAnnot": {
      "type": "obj",
      "title": "Annotation File with CDS Field",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "constraint": "type",
      "constraint_data": "u-ionAnnot",
      "_definer_type": "svc-profiler"
    },
    "scissors": {
      "type": "string",
      "default_value": "hiveseq",
      "title": "How to split the sequences",
      "description": "The way large jobs are split into smaller pieces",
      "order": 0,
      "is_readonly": false,
      "is_hidden": true,
      "constraint": "choice",
      "constraint_data": "hiveseq\/\/\/By the number of sequences|hiveal\/\/\/By the number of alignments",
      "_definer_type": "process"
    },
    "SEARCHFORREPEATS": {
      "type": "bool",
      "default_value": "0",
      "title": "Profile only repeated regions",
      "description": "Profile only regions which have alternate alignment sites",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "svc-profiler"
    },
    "snpCompare": {
      "type": "string",
      "default_value": "1",
      "title": "Profile type",
      "description": "The base sequence relative to which the SNP calling is performed",
      "order": 0,
      "is_key": true,
      "is_readonly": false,
      "constraint": "choice",
      "constraint_data": "1\/\/\/Relative to reference sequence|0\/\/\/Relative to consensus sequence",
      "_definer_type": "svc-profiler"
    },
    "split": {
      "type": "string",
      "default_value": "query",
      "title": "What field contains the object spliting the jobs",
      "order": 0,
      "is_readonly": false,
      "_definer_type": "process"
    },
    "splitList": {
      "type": "list",
      "title": "Split process into smaller chunks",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "is_flattened_decor": true,
      "is_hidden": true,
      "_definer_type": "process",
      "_children": {
        "nrepeat": {
          "type": "integer",
          "default_value": "1",
          "title": "Number of Computational Threads",
          "order": 0,
          "is_readonly": false,
          "is_optional": true,
          "is_hidden": true,
          "constraint": "eval",
          "constraint_data": "!isNaN(\"$_(val)\")",
          "constraint_description": "Give a Number",
          "_definer_type": "process"
            },
        "slice": {
          "type": "integer",
          "default_value": "500000",
          "title": "Number of Computational Subjects per Single Thread",
          "order": 0,
          "is_readonly": false,
          "is_hidden": true,
          "constraint": "eval",
          "constraint_data": "!isNaN(\"$_(val)\")",
          "constraint_description": "Give a Number",
          "_definer_type": "process"
            },
        "splitField": {
          "type": "string",
          "default_value": "parent_proc_ids",
          "title": "Split based on specified field(s)",
          "order": 0,
          "is_readonly": false,
          "is_hidden": true,
          "_definer_type": "svc-profiler"
            },
        "splitSize": {
          "type": "integer",
          "default_value": "100000",
          "title": "Number of alignments per single thread",
          "order": 0,
          "is_readonly": false,
          "is_batch": true,
          "constraint": "eval",
          "constraint_data": "!isNaN(\"$_(val)\")",
          "constraint_description": "Give a Number",
          "_definer_type": "svc-profiler"
            },
        "splitType": {
          "type": "string",
          "default_value": "alignments",
          "title": "Splitting function",
          "order": 4,
          "is_readonly": false,
          "is_hidden": true,
          "_definer_type": "svc-profiler"
            }
        }
    },
    "subSet": {
      "type": "string",
      "title": "Reference genome serial number",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "is_hidden": true,
      "_definer_type": "svc-dna"
    },
    "supportedDeletions": {
      "type": "bool",
      "default_value": "true",
      "title": "Include deletions in the coverage",
      "order": 0,
      "is_readonly": false,
      "is_optional": true,
      "_definer_type": "svc-profiler"
    },
    "system": {
      "type": "list",
      "title": "System",
      "order": 0,
      "is_readonly": false,
      "is_flattened_decor": true,
      "_definer_type": "process",
      "_children": {
        "action": {
          "type": "integer",
          "title": "Action for Request",
          "order": 0,
          "is_readonly": false,
          "is_hidden": true,
          "constraint": "choice",
          "constraint_data": "0\/\/\/Unknown|1\/\/\/Hold|2\/\/\/Run|3\/\/\/Terminate|4\/\/\/Suspend|5\/\/\/Resume",
          "_definer_type": "process"
            },
        "completed": {
          "type": "datetime",
          "title": "Completed",
          "order": 0,
          "is_readonly": true,
          "is_autofill": true,
          "_definer_type": "process"
        },
        "folder": {
          "type": "string",
          "default_value": "eval:typeof(user_get_curdir_save)!==\"undefined\"?user_get_curdir_save():\"\"",
          "title": "Destination folder",
          "order": 0,
          "is_readonly": false,
          "is_optional": true,
          "is_virtual": true,
          "is_weak_reference": true,
          "constraint": "type",
          "constraint_data": "folder+",
          "_definer_type": "process"
            },
        "isPostponed": {
          "type": "bool",
          "default_value": "false",
          "title": "Postpone execution",
          "description": "Launch job in suspended state, you will need to manually launching later",
          "order": 0,
          "is_readonly": false,
          "_definer_type": "process"
            },
        "onUserBehalf": {
          "type": "integer",
          "title": "On Behalf of The User",
          "order": 0,
          "is_readonly": false,
          "is_optional": true,
          "constraint": "search",
          "constraint_data": "{fetch:\"id\", inline:\"name\", outline:\"name\", url:\"http:/\/\/?cmd=usrList&active=1&grp=0&primaryGrpOnly=1&raw=1\"}",
          "_definer_type": "process"
            },
        "progress": {
          "type": "integer",
          "title": "Progress",
          "order": 0,
          "is_readonly": true,
          "is_autofill": true,
          "_definer_type": "process"
            },
        "reqID": {
          "type": "integer",
          "title": "Request ID",
          "order": 0,
          "is_readonly": true,
          "is_autofill": true,
          "_definer_type": "process"
            },
        "reqPriority": {
          "type": "integer",
          "default_value": "0",
          "title": "Priority of submission",
          "order": 0,
          "is_readonly": false,
          "is_optional": true,
          "constraint": "choice",
          "constraint_data": "0\/\/\/normal - execute ASAP in collaborative environment|100\/\/\/polite submission - let other users small computations to squeeze through|1000\/\/\/large submissions - do not compete with small and medium computations|1000000\/\/\/extremely nice - execute only when most computers are not busy",
          "_definer_type": "process"
            },
        "started": {
          "type": "datetime",
          "title": "Started",
          "order": 0,
          "is_readonly": true,
          "is_autofill": true,
          "_definer_type": "process"
            },
        "status": {
          "type": "integer",
          "title": "Status",
          "order": 0,
          "is_readonly": true,
          "is_autofill": true,
          "constraint": "choice",
          "constraint_data": "0\/\/\/Unknown|1\/\/\/Waiting|2\/\/\/Processing|3\/\/\/Running|4\/\/\/Suspended|5\/\/\/Done|6\/\/\/Killed|7\/\/\/ProgError|8\/\/\/SysError",
          "_definer_type": "process"
            },
        "svc": {
          "type": "string",
          "title": "Service ID",
          "order": 0,
          "is_readonly": true,
          "is_autofill": true,
          "_definer_type": "process"
            },
        "systemVersion": {
          "type": "string",
          "default_value": "eval:gSysVersion",
          "title": "System version number",
          "order": 0,
          "is_readonly": true,
          "is_autofill": true,
          "_definer_type": "process"
            },
        "svcTitle": {
          "type": "string",
          "title": "Service Title",
          "order": 2,
          "is_readonly": true,
          "is_autofill": true,
          "brief": "<b>$_(v)</b>",
          "_definer_type": "process"
            },
        "progress100": {
          "type": "integer",
          "title": "Progress %",
          "order": 3,
          "is_readonly": true,
          "is_autofill": true,
          "brief": "$_(v)%",
          "_definer_type": "process"
            }
        }
    }
  }
}
