              "order": 0,
              "is_readonly": false,
              "is_optional": true,
              "_definer_type": "process"
            }
          }
        },
        "batch_saturated": {
          "type": "bool",
          "title": "Saturated",
          "description": "Whether to force every batch for this list to contain at least one value from every parameter.",
