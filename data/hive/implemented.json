  "datetime"  :  1,    // string representing a valid combined date & time in ISO 8601 format, e.g. "2017  :  1,    //08  :  1,    //24T19:32:30Z" (timezone optional)
  "url"       :  1,    // string containing a valid URL
  "objectid"  :  1,    // object ID (see format spec above)
  "password"  :  1,    // on input, string containing a password; on output, a placeholder, e.g. \"*****\" or similar
